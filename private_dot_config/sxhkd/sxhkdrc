# terminal emulator
super + Return
	kitty

super + e
	nautilus

super + @space
	rofi -show combi -display-combi "" -display-drun "" -display-window "" -combi-modi drun,window

super + r
	rofi -show run -display-run "" 

super + Delete
	xkill
	
# Reload configuration
super + Escape
	pkill -USR1 -x sxhkd

Print
    deepin-screenshot


super + ctrl + q
	bspc quit
